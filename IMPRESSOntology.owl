    </owl:ObjectProperty>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology/causes -->

    <owl:ObjectProperty rdf:about="&impress;causes">
        <rdfs:comment xml:lang="en">A Person may cause an EOPHC by a specific action. ie. A Person may cayuse a wild fire. Also, an EOPHC may cause another EOPHC ie. the collapse of a building may cause a fire in the building.</rdfs:comment>
        <rdfs:range rdf:resource="http://fp7-impress.eu/EOPHC/EOPHC"/>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/EOPHC/EOPHC"/>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/Person/IMPRESSPerson"/>
    </owl:ObjectProperty>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology/controls -->

    <owl:ObjectProperty rdf:about="&impress;controls">
        <rdfs:comment xml:lang="en">A Person (or a group of people) under specific roles (ie. rescue team coordinator) executes an Activity which controls the range of an EOPHC.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/Activity/IMPRESSActivity"/>
        <rdfs:range rdf:resource="http://fp7-impress.eu/EOPHC/EOPHC"/>
    </owl:ObjectProperty>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology/executes -->

    <owl:ObjectProperty rdf:about="&impress;executes">
        <rdfs:comment xml:lang="en">A Person (or a group of people) executes an Activity in order to respond to an EOPHC.</rdfs:comment>
        <rdfs:range rdf:resource="http://fp7-impress.eu/Activity/IMPRESSActivity"/>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/Person/IMPRESSPerson"/>
    </owl:ObjectProperty>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology/impact -->

    <owl:ObjectProperty rdf:about="&impress;impact">
        <rdfs:comment xml:lang="en">The impact that an EOPHC may have on a group of people. An EOPHC has an effect and influences a Person in a specific way.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/EOPHC/EOPHC"/>
        <rdfs:range rdf:resource="http://fp7-impress.eu/Person/IMPRESSPerson"/>
    </owl:ObjectProperty>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology/uses -->

    <owl:ObjectProperty rdf:about="&impress;uses">
        <rdfs:comment xml:lang="en">An Activity uses a Resource in order to control the range of an EOPHC.</rdfs:comment>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/Activity/IMPRESSActivity"/>
        <rdfs:range rdf:resource="http://fp7-impress.eu/Resource/IMPRESSResource"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="&wgs84_pos;location">
        <rdfs:label>location</rdfs:label>
        <rdfs:comment>The relation between something and the point, 
 or other geometrical thing in space, where it is.  For example, the realtionship between
 a radio tower and a Point with a given lat and long.
 Or a relationship between a park and its outline as a closed arc of points, or a road and
 its location as a arc (a sequence of points).
 Clearly in practice there will be limit to the accuracy of any such statement, but one would expect
 an accuracy appropriate for the size of the object and uses such as mapping .
 </rdfs:comment>
        <rdfs:range rdf:resource="&wgs84_pos;SpatialThing"/>
        <rdfs:subPropertyOf rdf:resource="&foaf;based_near"/>
    </owl:ObjectProperty>
    


