    <!-- http://fp7-impress.eu/IMPRESSOntology#Black -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#Black">
        <rdf:type rdf:resource="http://fp7-impress.eu/Activity/Triage"/>
        <skos:prefLabel rdf:datatype="&xsd;string">Black</skos:prefLabel>
        <rdfs:comment rdf:datatype="&xsd;string">Black / Expectant: Severely injured that they will die of their injuries, possibly in hours or days (large-area burns, severe trauma, lethal radiation dose), or in life-threatening medical crisis that they are unlikely to survive given the care available (cardiac arrest, septic shock, severe head or chest wounds); their treatment is usually palliative, such as being given painkillers, to reduce suffering.</rdfs:comment>
        <skos:inScheme rdf:resource="http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode"/>
    </owl:NamedIndividual>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology#Green -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#Green">
        <rdf:type rdf:resource="http://fp7-impress.eu/Activity/Triage"/>
        <skos:prefLabel rdf:datatype="&xsd;string">Green</skos:prefLabel>
        <rdfs:comment rdf:datatype="&xsd;string">Green / Wait (walking wounded): People will require a doctor&apos;s care in several hours or days but not immediately, may wait for a number of hours or be told to go home and come back the next day (broken bones without compound fractures, many soft tissue injuries).</rdfs:comment>
        <skos:inScheme rdf:resource="http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode"/>
    </owl:NamedIndividual>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology#PalermoPublicBuilding -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#PalermoPublicBuilding">
        <rdf:type rdf:resource="&skos;ConceptScheme"/>
    </owl:NamedIndividual>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology#Red -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#Red">
        <rdf:type rdf:resource="http://fp7-impress.eu/Activity/Triage"/>
        <skos:prefLabel rdf:datatype="&xsd;string">Red</skos:prefLabel>
        <rdfs:comment rdf:datatype="&xsd;string">Red / Immediate: People require immediate surgery or other life-saving intervention, and have first priority for surgical teams or transport to advanced facilities; they &quot;cannot wait&quot; but are likely to survive with immediate treatment.</rdfs:comment>
        <skos:inScheme rdf:resource="http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode"/>
    </owl:NamedIndividual>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode">
        <rdf:type rdf:resource="http://fp7-impress.eu/Activity/Triage"/>
        <rdf:type rdf:resource="&skos;ConceptScheme"/>
    </owl:NamedIndividual>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology#White -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#White">
        <rdf:type rdf:resource="http://fp7-impress.eu/Activity/Triage"/>
        <skos:prefLabel rdf:datatype="&xsd;string">White</skos:prefLabel>
        <rdfs:comment rdf:datatype="&xsd;string">White / Dismiss (walking wounded): People have minor injuries; first aid and home care are sufficient, a doctor&apos;s care is not required. Injuries are along the lines of cuts and scrapes, or minor burns.</rdfs:comment>
        <skos:inScheme rdf:resource="http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode"/>
    </owl:NamedIndividual>
    


    <!-- http://fp7-impress.eu/IMPRESSOntology#Yellow -->

    <owl:NamedIndividual rdf:about="http://fp7-impress.eu/IMPRESSOntology#Yellow">
        <rdf:type rdf:resource="http://fp7-impress.eu/Activity/Triage"/>
        <skos:prefLabel rdf:datatype="&xsd;string">Yellow</skos:prefLabel>
        <rdfs:comment rdf:datatype="&xsd;string">Yellow / Observation: People&apos;s condition is stable for the moment but requires watching by trained persons and frequent re-triage, will need hospital care (and would receive immediate priority care under &quot;normal&quot; circumstances).</rdfs:comment>
        <skos:inScheme rdf:resource="http://fp7-impress.eu/IMPRESSOntology#TriageSchemeCode"/>
    </owl:NamedIndividual>
    


