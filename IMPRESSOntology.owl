    <!-- http://fp7-impress.eu/IMPRESSOntology#hasReport -->

    <owl:ObjectProperty rdf:about="http://fp7-impress.eu/IMPRESSOntology#hasReport">
        <rdfs:range rdf:resource="http://fp7-impress.eu/IMPRESSOntology#HospitalReport"/>
        <rdfs:domain rdf:resource="http://fp7-impress.eu/Resource/Hospital"/>
    </owl:ObjectProperty>
    


